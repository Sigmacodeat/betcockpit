{
  "name": "provider-proxy",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "start": "node dist/server.js",
    "start:prod": "NODE_ENV=production node dist/server.js",
    "clean": "rimraf dist",
    "build": "tsc -p tsconfig.json",
    "build:clean": "npm run clean && npm run build",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name init_dev_email",
    "prisma:studio": "prisma studio",
    "postinstall": "prisma generate",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --config ./jest.config.mjs"
  },
  "dependencies": {
    "@prisma/client": "^5.16.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^8.0.1",
    "fast-xml-parser": "^4.3.6",
    "helmet": "^7.1.0",
    "http-proxy-middleware": "^3.0.3",
    "imapflow": "^1.0.164",
    "ioredis": "^5.4.1",
    "morgan": "^1.10.0",
    "stripe": "^14.25.0",
    "unzipper": "^0.12.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.12.12",
    "@types/supertest": "^6.0.3",
    "jest": "^29.7.0",
    "prisma": "^5.16.2",
    "rimraf": "^6.0.1",
    "supertest": "^7.1.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.1",
    "typescript": "^5.5.4"
  }
}
